#include <iostream>
#include <string>
#include <cctype>

using namespace std;
string transformar_em_minusculas(string s) {
    string nova_s;
    for (int i = 0; i < s.length(); i++) {
        nova_s += tolower(s[i]);
    }
    return nova_s;
}
string alfanum(string s){
    string nova_s;
    for (int i = 0; i < s.length(); i++) {
        if(isalnum(s[i])){
            nova_s += s[i];
        }
    }
    return nova_s;
}
